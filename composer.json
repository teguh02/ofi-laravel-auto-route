{
    "name": "ofi/laravel-auto-route",
    "description": "This package can make laravel to auto route",
    "license": "MIT",
    "authors": [
        {
            "name": "Teguh Rijanandi",
            "email": "teguhrijanandi02@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "keywords": ["Laravel", "Auto Route"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Ofi\\Route\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ofi\\Route\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ofi\\Route\\OfiServiceProvider"
            ],
            "aliases": {
                "AutoRoute": "Ofi\\Route\\Facades\\AutoRoute"
            }
        }
    }
}
